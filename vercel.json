{
  "version": 2,
  "name": "ai-business-automation-tree",
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "main.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "main.py"
    },
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  },
  "regions": ["iad1"],
  "functions": {
    "main.py": {
      "maxDuration": 60
    }
  }
}
